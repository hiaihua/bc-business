<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 公司文件 -->
	<package name="companyFile" extends="bc-default" namespace="/bc-business/companyFile">
		<action name="create" class="companyFileAction" method="create">
			<result name="form">/bc-business/companyFile/form.jsp</result>
		</action>
		<action name="edit" class="companyFileAction" method="edit">
			<result name="form">/bc-business/companyFile/form.jsp</result>
		</action>
		<action name="open" class="companyFileAction" method="open">
			<result name="formr">/bc-business/companyFile/formr.jsp</result>
		</action>
		<action name="save" class="companyFileAction" method="save"/>
		<action name="issue" class="companyFileAction" method="issue"/>
		<action name="delete" class="companyFileAction" method="delete"/>
	</package>
</struts>