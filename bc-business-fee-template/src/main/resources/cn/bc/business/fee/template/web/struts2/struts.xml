<struts>
	
	<package name="feeTemplate" extends="bc-default" namespace="/bc-business/feeTemplate">
		<action name="create" class="feeTemplateAction" method="create">
			<result name="form">/bc-business/feeTemplate/form.jsp</result>
		</action>		
