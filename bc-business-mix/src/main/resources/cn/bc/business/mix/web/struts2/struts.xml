<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="infoCenter" extends="bc-default" namespace="/bc-business/infoCenter">
		<action name="main" class="infoCenterAction" method="execute">
			<result name="success">/bc-business/infoCenter/main.jsp
			</result>
		</action>
		<action name="findCarsByUnitAndMotorcade" class="infoCenterAction"
			method="findCarsByUnitAndMotorcade" />
		<action name="findCarsBySearchText" class="infoCenterAction"
			method="findCarsBySearchText" />
		<action name="findCarDetail" class="infoCenterAction" method="findCarDetail" />
	</package>
</struts>